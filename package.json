{
    "name": "@jwrae/design-system",
    "version": "1.0.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "A responsive Vue 3 design system with Tailwind CSS and Storybook",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:with-types": "vue-tsc && vite build",
        "build:lib": "vite build --config vite.lib.config.js",
        "preview": "vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "serve-storybook": "npx http-server storybook-static"
    },
    "dependencies": {
        "@headlessui/vue": "^1.7.16",
        "@heroicons/vue": "^2.0.18",
        "vue": "^3.3.8"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.5.3",
        "@storybook/addon-interactions": "^7.5.3",
        "@storybook/addon-links": "^7.5.3",
        "@storybook/blocks": "^7.5.3",
        "@storybook/testing-library": "^0.2.2",
        "@storybook/vue3": "^7.5.3",
        "@storybook/vue3-vite": "^7.5.3",
        "@types/node": "^20.8.10",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.9.1",
        "@vitejs/plugin-vue": "^4.4.1",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.53.0",
        "eslint-plugin-storybook": "^0.6.15",
        "eslint-plugin-vue": "^9.18.1",
        "postcss": "^8.4.31",
        "prettier": "^3.0.3",
        "storybook": "^7.5.3",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2",
        "vite": "^4.5.0",
        "vite-plugin-dts": "^3.6.3",
        "vue-tsc": "^1.8.22"
    },
    "peerDependencies": {
        "vue": "^3.3.0"
    },
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./style.css": "./dist/style.css"
    },
    "keywords": [
        "vue3",
        "design-system",
        "component-library",
        "tailwindcss",
        "typescript",
        "storybook"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jw-rae/vue3-design-system.git"
    },
    "license": "MIT"
}